Xft.dpi:		96
Xft.antialias:		true
Xft.rgba:		true
Xft.hinting:		true
Xft.hintstyle:		hintslight

URxvt.scrollBar: 	false
URxvt*font: 		xft:SauceCodePro Nerd Font Mono:size=11
URxvt*boldFont: 	xft:SauceCodePro Nerd Font Mono:style=Semibold:size=11

URxvt.cursorBlink:	true
URxvt.cursorColor:	#ffcc00
URxvt.cursorUnderline:	false
URxvt.pointerBlank:	true

URxvt.background: 	#263238
*foreground: 		#eeffff
*foreground_bold: 	#f1ffff

